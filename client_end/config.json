{
  "log" : {
    "loglevel" : "warning",
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log"
  },
  "dns": {
    "servers": [
      "114.114.114.114",
      {
        "address": "1.1.1.1",
        "port": 53,
        "domains": [
          "geosite:geolocation-!cn"
        ]
      }
    ]
  },
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "outboundTag": "direct", 
        "domain": ["geosite:cn"] 
      },
      {
        "type": "field",
        "outboundTag": "direct", 
        "ip": [
          "geoip:cn", 
          "geoip:private"
        ]
      },
      {
        "type": "field",
        "outboundTag": "proxy",
        "network": "udp,tcp"
      }  
    ]
  },
  "inbounds" : [
    {
      "port": 10800,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "udp": true
      }
    }
  ],
  "outbounds" : [
    {
      "mux" : {
        "enabled" : true,
        "concurrency" : 8
      },
      "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "18.191.216.55", 
                        "port": 443,
                        "users": [
                            {
                                "id": "e3325ffc-1532-4aac-a8a9-455430452599",
                                "encryption": "none",
                                "level": 0
                            }
                        ]
                    }
                ]
            },
      "streamSettings" : {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
            "serverName": "ericbai.top"
        },
        "wsSettings": {
            "path": "/websocket" 
        }
      }
    }
  ]
}